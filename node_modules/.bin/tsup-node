#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/tsup@8.5.0_jiti@2.4.2_postcss@8.5.6_tsx@4.19.1_typescript@5.7.3_yaml@2.7.1/node_modules/tsup/dist/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/tsup@8.5.0_jiti@2.4.2_postcss@8.5.6_tsx@4.19.1_typescript@5.7.3_yaml@2.7.1/node_modules/tsup/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/tsup@8.5.0_jiti@2.4.2_postcss@8.5.6_tsx@4.19.1_typescript@5.7.3_yaml@2.7.1/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/tsup@8.5.0_jiti@2.4.2_postcss@8.5.6_tsx@4.19.1_typescript@5.7.3_yaml@2.7.1/node_modules/tsup/dist/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/tsup@8.5.0_jiti@2.4.2_postcss@8.5.6_tsx@4.19.1_typescript@5.7.3_yaml@2.7.1/node_modules/tsup/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/tsup@8.5.0_jiti@2.4.2_postcss@8.5.6_tsx@4.19.1_typescript@5.7.3_yaml@2.7.1/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
